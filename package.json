{
  "name": "flash-mobile-boilerplate",
  "version": "0.0.1",
  "private": true,
  "description": "Flash mobile development boilerplate",
  "author": "Vladimir Lopez <vlopezsalvador.22@gmail.com>",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "NODE_ENV=production build-storybook",
    "dev": "vite --host",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "postinstall": "chmod +x .devfix/vite.sh; .devfix/vite.sh",
    "ios": "yarn build; cap sync ios",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --ignore-path ./.gitignore",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 safari version",
      "not IE 9-11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "jsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./src/setupTest.ts"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx}",
      "<rootDir>/src/components/**/__tests__/?(*.)+(spec|test).tsx?(x)"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest"
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic)"
    ]
  },
  "dependencies": {
    "@capacitor-community/http": "^1.4.1",
    "@capacitor/android": "^3.5.0",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.5.0",
    "@capacitor/device": "^1.1.2",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "^3.5.0",
    "@capacitor/keyboard": "^1.2.2",
    "@capacitor/local-notifications": "^1.1.0",
    "@ionic/react": "^6.0.0",
    "@ionic/react-router": "^6.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "clsx": "^1.1.1",
    "fuzzy-search": "^3.2.1",
    "ionicons": "^5.4.0",
    "moment": "^2.29.3",
    "pretty-bytes": "^5.6.0",
    "pretty-ms": "^7.0.1",
    "ramda": "^0.28.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.31.1",
    "react-redux": "^8.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-observable": "^1.2.0",
    "redux-persist-capacitor": "^1.0.2",
    "redux-persist-capacitor-storage": "^1.0.8",
    "reduxjs-toolkit-persist": "^7.0.1",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.7",
    "simpl-schema": "^1.12.0",
    "socket.io-client": "^2.2.0",
    "swiper": "^8.1.4",
    "web-vitals": "^2.1.2",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.5.0",
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.1.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.19.15",
    "@types/ramda": "^0.28.11",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/simpl-schema": "^1.10.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.3.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^28.0.2",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  }
}
